{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "cucumber-js",
    "test:allure": "cucumber-js --format @cucumber/pretty-formatter --format allure-cucumberjs",
    "report:generate": "allure generate ./allure-results --clean",
    "report:open": "allure open ./allure-report",
    "test:report": "npm run test:allure && npm run report:generate && npm run report:open",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:accessibility": "cucumber-js --tags @accessibility",
    "test:api": "cucumber-js --tags @api",
    "test:performance": "cucumber-js --tags @performance",
    "lighthouse": "lighthouse"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.5",
    "@cucumber/cucumber": "^10.3.1",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.42.1",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "allure-commandline": "^2.27.0",
    "allure-cucumberjs": "^2.12.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "lighthouse": "^11.6.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}